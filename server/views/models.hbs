<div class="card m-3">
    <div class="card-body p-3">
        <h3 class="card-title">Available Models</h3>
        {{#each models as | model |}}
        <div class="card mt-3">
            <div class="card-body p-3">
                <div class="row">
                    <div class="col-11">
                        <h4 class="card-title">{{ model.name }}</h4>
                    </div>
                    <div class="col-1">
                        <a class="btn btn-primary" href="#">Download Zip</a>
                    </div>
                </div>
                 <h6 class="card-title">Model Data</h6>
                 {{#each model.resources as | resource |}}
                 <div class="card mt-2 p-2">
                    Filename: {{ resource.name }} <br>
                    Size: {{ resource.size }}B <br>
                    LastModified: {{ resource.lastModified }} <br>
                 </div>
                 {{/each}}
                
            </div>
        </div>
        {{/each}}
    </div>
</div>
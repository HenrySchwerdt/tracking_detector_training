<div class="card m-3">
  <div class="card-body p-3">
     <h3 class="card-title">Job Overview</h3>
        {{#each jobs as | job |}}
            <div class="card mt-3">
                 <div class="card-body p-3">
                    <h6 class="card-title">{{ job.jobName }}</h6>
                     <p class="card-text">{{ job.jobDescription }}</p>
                     cronPattern: {{ job.cronPattern }} <br>
                     enabled: {{ job.enabled }}
                 </div>
            </div>
        {{/each}} 
  </div>
</div>
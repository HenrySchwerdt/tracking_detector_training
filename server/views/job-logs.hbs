<div class="card m-3">
    <div class="card-body p-3">
        <h3 class="card-title">Logs Overview : {{jobRun.id}}</h3>
        <div class="card mt-3">
            <div class="row p-3">
                <div class="col-3">
                    <h6 class="card-title">RunId: {{ jobRun.id }}</h6>
                    startTime: {{#formatDate jobRun.startTime}}{{/formatDate}} <br>
                    endTime: {{#formatDate jobRun.endTime}}{{/formatDate}} <br>
                    status: {{jobRun.status}}
                </div>
                <div class="col-9">
                    <textarea style="overflow:auto;resize:none" class="form-control" aria-label="Logs" readonly rows="20">
                        {{jobRun.logs}}
                    </textarea>
                </div>
            </div>
        </div>
    </div>
</div>